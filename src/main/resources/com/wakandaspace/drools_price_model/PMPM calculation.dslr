package com.wakandaspace.drools_price_model;

import function com.wakandaspace.drools_price_model.BenefitUtil.getCost;
import function com.wakandaspace.drools_price_model.BenefitUtil.getUtilisationPerBenefit;

rule "PMPM calculation"
salience 20
    when
        There is a plan 
        and There is a benefit
    then
       get the cost of the benefit
       get the use of the benefit
       set the value of PMPM in calculationResult
end
